{
  "name": "@openclaw/wemp",
  "version": "0.2.0",
  "description": "Officially loadable OpenClaw channel plugin for WeChat Official Account",
  "type": "module",
  "main": "index.js",
  "files": [
    "index.js",
    "openclaw.plugin.json",
    "README.md"
  ],
  "scripts": {},
  "keywords": [
    "openclaw",
    "wechat",
    "official-account",
    "gateway-plugin"
  ],
  "peerDependencies": {
    "openclaw": "*"
  },
  "openclaw": {
    "extensions": [
      "./index.js"
    ],
    "channel": {
      "id": "wemp",
      "label": "wemp (WeChat Official Account)",
      "selectionLabel": "wemp (WeChat Official Account Webhook)",
      "detailLabel": "wemp",
      "docsPath": "/channels/wemp",
      "docsLabel": "wemp",
      "blurb": "Connect WeChat Official Account webhook messages to OpenClaw Gateway.",
      "aliases": [
        "wechat",
        "wemp",
        "weixin",
        "公众号",
        "微信"
      ],
      "order": 1
    },
    "install": {
      "npmSpec": "git+https://github.com/zuoa/openclaw-wemp.git",
      "localPath": "~/openclaw-wemp",
      "defaultChoice": "local"
    }
  },
  "dependencies": {},
  "devDependencies": {}
}
